<h1> Listing <%= pluralize(@dojos.count, "Dojo") %> </h1>

<% if flash[:notice] %>
    <p><%= flash[:notice] %></p>
<% end %>

<h1>Dojo Listings</h1>

<table>
	<thead>
		<th>Coding Dojo Branch</th>
		<th>Street</th>
		<th>City</th>
		<th>State</th>
	</thead>
	<tbody>
		<% @dojos.each do |dojo| %>
		<tr>
			<td><%= dojo.branch %></td>
			<td><%= dojo.street %></td>
			<td><%= dojo.city %></td>
			<td><%= dojo.state %></td>
		</tr>
		<% end %>
	</tbody>
</table>

<%= link_to "New Dojo", new_dojo_path %>
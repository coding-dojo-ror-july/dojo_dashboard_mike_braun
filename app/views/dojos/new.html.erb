<% if flash[:errors] %>
    <% flash[:errors].each do |e| %>
        <p><%= e %></p>
    <% end %>
<% end %>

<h1>New Dojo</h1>
<form action='/dojos' method='post'>
<input name="authenticity_token" value="<%= form_authenticity_token %>" type="hidden">
<h3>Branch: <input type='text' name='dojo[branch]'></h3>
<h3>Street: <input type='text' name='dojo[street]'></h3>
<h3>City: <input type='text' name='dojo[city]'></h3>
<h3>State: <input type='text' name='dojo[state]'></h3>

<input type="submit" value= "Create">

<%= link_to "Back", root_path %>

